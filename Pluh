-- Fake Module Scripts:

local fake_module_scripts = {}


-- Fake Local Scripts:

local function TSGBJDZ_fake_script() -- Fake Script: StarterGui.SaladINTERNAL.DarkShit.PanelMAIN.CodeExecution.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_CodeExecution"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local tabshit = script.Parent
	shared.CodeExecutionToggled = false 
	local TS = game:GetService("TweenService")
	local ti = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out) 
	local pcstat = tabshit.Parent.PCStat 
	local localplayer = tabshit.Parent.LocalPlayer 
	local devconsole = tabshit.Parent.DevConsole 
	local builtinshit = tabshit.Parent.BuiltInScripts 
	local buildinfo = tabshit.Parent.BuildInfo 
	local codeexec = tabshit.Parent.Tabs.Coding 
	
	function tweenc(object, properties)
		local tween = TS:Create(object, ti, properties)
		tween:Play()
		return tween
	end
	
	tabshit.MouseButton1Click:Connect(function()
		if shared.CodeExecutionToggled then
			tweenc(codeexec, {Transparency = 1}) 
			wait(ti.Time) 
			codeexec.Visible = false
			tweenc(tabshit, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
			tweenc(tabshit, {ImageColor3 = Color3.fromRGB(200, 200, 200)})
			shared.CodeExecutionToggled = false 
		else
			codeexec.Visible = true 
			tweenc(codeexec, {Transparency = 0})
			tweenc(tabshit, {BackgroundColor3 = Color3.fromRGB(255, 255, 255)})
			tweenc(tabshit, {ImageColor3 = Color3.fromRGB(0, 0, 0)})
			tweenc(pcstat, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
			tweenc(pcstat, {ImageColor3 = Color3.fromRGB(200, 200, 200)})
			tweenc(localplayer, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
			tweenc(localplayer, {ImageColor3 = Color3.fromRGB(200, 200, 200)})
			tweenc(devconsole, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
			tweenc(devconsole, {ImageColor3 = Color3.fromRGB(200, 200, 200)})
			tweenc(builtinshit, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
			tweenc(builtinshit, {ImageColor3 = Color3.fromRGB(200, 200, 200)})
			tweenc(buildinfo, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
			tweenc(buildinfo, {ImageColor3 = Color3.fromRGB(200, 200, 200)})
	
			shared.CodeExecutionToggled = true  
			shared.BuildInfoToggled = false  
			shared.BuiltInScripts = false 
			shared.DevConsoleToggled = false
			shared.LocalPlayuerToggled = false 
			shared.PCStatToggled = false 
		end
	end)
	
end
local function LGAXU_fake_script() -- Fake Script: StarterGui.SaladINTERNAL.DarkShit.PanelMAIN.LocalPlayer.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_LocalPlayer"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local tabshit = script.Parent
	shared.LocalPlayuerToggled = false 
	local TS = game:GetService("TweenService")
	local ti = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out) 
	local pcstat = tabshit.Parent.PCStat 
	local localplayer = tabshit.Parent.CodeExecution 
	local devconsole = tabshit.Parent.DevConsole 
	local builtinshit = tabshit.Parent.BuiltInScripts 
	local buildinfo = tabshit.Parent.BuildInfo 
	function tweenc(object, properties)
		local tween = TS:Create(object, ti, properties)
		tween:Play()
	end
	tabshit.MouseButton1Click:Connect(function()
		if shared.LocalPlayuerToggled then
			shared.LocalPlayuerToggled = false 
			tweenc(tabshit, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
			tweenc(tabshit, {ImageColor3 = Color3.fromRGB(200, 200, 200)})
		else
			shared.LocalPlayuerToggled = true  
			tweenc(tabshit, {BackgroundColor3 = Color3.fromRGB(255, 255, 255)})
			tweenc(tabshit, {ImageColor3 = Color3.fromRGB(0, 0, 0)})
			tweenc(pcstat, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)})
