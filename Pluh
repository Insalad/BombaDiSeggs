local function VPPFAR_fake_script() -- Fake Script: StarterGui.SaladINTERNAL.DarkShit.PanelMAIN.Tabs.Coding.DragBlah
    local script = Instance.new("LocalScript")
    script.Name = "DragBlah"
    script.Parent = Converted["_Coding"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local UIS = game:GetService("UserInputService")
	local par = script.Parent
	local isdr
	local drin
	local drst
	local stpos
	function update(input)
		local delta = input.Position - drst
		par.Position = UDim2.new(stpos.X.Scale, stpos.X.Offset + delta.X, stpos.Y.Scale, stpos.Y.Offset + delta.Y)
	end
	par.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			isdr = true
			drst = input.Position
			stpos = par.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					isdr = false
				end
			end)
		end
	end)
	par.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			drin = input
		end
	end)
	UIS.InputChanged:Connect(function(input)
		if input == drin and isdr then
			update(input)
		end
	end)
end
local function HKEYOL_fake_script() -- Fake Script: StarterGui.SaladINTERNAL.DarkShit.PanelMAIN.Tabs.Coding.TextBox.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_TextBox"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

local textbox = script.Parent 
local highlight = Instance.new("TextLabel", textbox.Parent)
highlight.Size = textbox.Size
highlight.Position = textbox.Position
highlight.BackgroundTransparency = 1
highlight.TextXAlignment = textbox.TextXAlignment
highlight.TextYAlignment = textbox.TextYAlignment
highlight.Font = textbox.Font
highlight.RichText = true
highlight.TextScaled = false
highlight.TextSize = 20
highlight.Text = "--Paste your scripts here"
highlight.TextColor3 = Color3.fromRGB(100, 100, 100)  
highlight.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
highlight.BackgroundTransparency = 0.8 
highlight.TextWrapped = true
highlight.TextColor3 = Color3.new(1, 1, 1)  
textbox.TextTransparency = 0 
textbox.BackgroundTransparency = 1

local cursor = Instance.new("TextLabel", textbox.Parent)
cursor.Size = UDim2.new(0, 20, 1, 0) 
cursor.Position = UDim2.new(0, 0, 0, 0)
cursor.BackgroundTransparency = 1
cursor.TextColor3 = Color3.new(1, 1, 1)
cursor.Text = "|"
cursor.Visible = false

local cursorvi = false
local function togglec()
	cursorvi = not cursorvi
	cursor.Visible = cursorvi
end

spawn(function() 
	while true do
		togglec()
		wait(0.5) 
	end
end)

local keywords = {
	["local"] = Color3.fromRGB(0, 121, 184),        
	["function"] = Color3.fromRGB(189, 98, 255),    
	["if"] = Color3.fromRGB(198, 77, 255),          
	["then"] = Color3.fromRGB(0, 175, 239),         
	["end"] = Color3.fromRGB(189, 98, 255),         
	["for"] = Color3.fromRGB(0, 100, 255),          
	["while"] = Color3.fromRGB(0, 130, 255),        
	["do"] = Color3.fromRGB(0, 150, 255),           
	["return"] = Color3.fromRGB(189, 98, 255),      
	["true"] = Color3.fromRGB(0, 121, 184),         
	["false"] = Color3.fromRGB(0, 121, 184),        
}

local renv = {
	print, warn, error, assert, collectgarbage, load, require, select, tonumber, tostring, type, xpcall, pairs, next, ipairs,
	newproxy, rawequal, rawget, rawset, rawlen, setmetatable, PluginManager,
	coroutine.create, coroutine.resume, coroutine.running, coroutine.status, coroutine.wrap, coroutine.yield,
	bit32.arshift, bit32.band, bit32.bnot, bit32.bor, bit32.btest, bit32.extract, bit32.lshift, bit32.replace, bit32.rshift, bit32.xor,
	math.abs, math.acos, math.asin, math.atan, math.atan2, math.ceil, math.cos, math.cosh, math.deg, math.exp, math.floor, math.fmod, math.frexp, math.ldexp, math.log, math.log10, math.max, math.min, math.modf, math.pow, math.rad, math.random, math.randomseed, math.sin, math.sinh, math.sqrt, math.tan, math.tanh,
	string.byte, string.char, string.find, string.format, string.gmatch, string.gsub, string.len, string.lower, string.match, string.pack, string.packsize, string.rep, string.reverse, string.sub, string.unpack, string.upper,
	table.concat, table.insert, table.pack, table.remove, table.sort, table.unpack,
	utf8.char, utf8.charpattern, utf8.codepoint, utf8.codes, utf8.len, utf8.nfdnormalize, utf8.nfcnormalize,
	os.clock, os.date, os.difftime, os.time,
	delay, elapsedTime, require, spawn, tick, time, typeof, UserSettings, version, wait,
	task.defer, task.delay, task.spawn, task.wait,
	debug.traceback, debug.profilebegin, debug.profileend
}

for _, func in ipairs(renv) do
	local funcname = debug.info(func, "n")  
	if funcname then
		keywords[funcname] = Color3.fromRGB(255, 255, 85)  
	end
end

function syntaxhighlight(text)
	local yestext = text
	yestext = string.gsub(yestext, "\"([^\"]*)\"", "<font color=\"rgb(255, 165, 0)\">\"%1\"</font>")  
	for keyword, color in pairs(keywords) do
		yestext = string.gsub(yestext, "%f[%a]" .. keyword .. "%f[%A]", "<font color=\"rgb(".. tostring(math.floor(color.r * 255)) .. ",".. tostring(math.floor(color.g * 255)) .. ",".. tostring(math.floor(color.b * 255)) .. ")\">" .. keyword .. "</font>")
	end
	return yestext
end

textbox:GetPropertyChangedSignal("Text"):Connect(function()
	local text = textbox.Text
	if #text > 200000 then  
		highlight.Text = text  
	else
		local yestext = syntaxhighlight(text)
		highlight.Text = yestext
	end
	local txtsixe = highlight.TextSize
	local cxpos = txtsixe
	cursor.Position = UDim2.new(0, cxpos, 0, 0) 
end)

textbox:GetPropertyChangedSignal("Text"):Connect(function()
	cursor.Visible = true
end)

textbox.FocusLost:Connect(function()
	cursor.Visible = false
end)

textbox.Focused:Connect(function()
	cursor.Visible = true
end)
end
