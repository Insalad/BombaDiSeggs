local function AQCCPJF_fake_script() -- Fake Script: StarterGui.SaladINTERNAL.UIToggle
    local script = Instance.new("LocalScript")
    script.Name = "UIToggle"
    script.Parent = Converted["_SaladINTERNAL"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local TS = game:GetService("TweenService")
	local blur = script.Parent.DarkShit
	local panel = blur.PanelMAIN
	local UIS = game:GetService("UserInputService")
	local showing = true
	local TINFO = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local bto = TS:Create(blur, TweenInfo.new(0.4), {Transparency = 1})
	local bti = TS:Create(blur, TweenInfo.new(0.4), {Transparency = 0.4})
	local pto = TS:Create(panel, TweenInfo.new(0.5), {Transparency = 1})
	local pti = TS:Create(panel, TweenInfo.new(0.5), {Transparency = 0})
	local shadow = panel.DropShadowHolder.DropShadow
	local codeexec = panel.CodeExecution
	local pcstat = panel.PCStat
	local localplr = panel.LocalPlayer
	local console = panel.DevConsole
	local builtinshit = panel.BuiltInScripts
	local buildinfo = panel.BuildInfo
	function buttonshittoggleblah(show)
		local ttrnsp = show and 0 or 1
		for _, button in ipairs({codeexec, pcstat, localplr, console, builtinshit, buildinfo}) do
			local btween = TS:Create(button, TINFO, {ImageTransparency = ttrnsp})
			btween:Play()
			if button.BackgroundTransparency then
				local btween2 = TS:Create(button, TINFO, {BackgroundTransparency = ttrnsp})
				btween2:Play()
			end
		end
	end
	function shadowtoggle(show)
		local shadowt = TS:Create(shadow, TINFO, {ImageTransparency = show and 0.5 or 1})
		shadowt:Play()
	end
	function toggleBlurAndPanel(input, gpr)
		if not gpr and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.RightShift then
			if showing then 
				showing = false 
				bto:Play()
				pto:Play()
				buttonshittoggleblah(false)
				shadowtoggle(false)
	
				bto.Completed:Wait()
				pto.Completed:Wait()
			else 
				showing = true 
				blur.Visible = true 
				panel.Visible = true 
				buttonshittoggleblah(true)
				shadowtoggle(true)
	
				bti:Play()
				pti:Play()
			end
		end
	end
	UIS.InputBegan:Connect(
		function(input, gpr)
			if not gpr and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.RightShift then
				if showing then 
					showing = false 
					bto:Play()
					pto:Play()
					buttonshittoggleblah(false)
					shadowtoggle(false)
	
					bto.Completed:Wait()
					pto.Completed:Wait()
				else 
					showing = true 
					blur.Visible = true 
					panel.Visible = true 
					buttonshittoggleblah(true)
					shadowtoggle(true)
	
					bti:Play()
					pti:Play()
				end
			end
		end 
	)
	
end

